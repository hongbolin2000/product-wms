        <input type="text" name="name" expr="rolenm" title="@ROLEMS.ROLENM" required="true" length="32"/>
        <input type="text" name="define01" expr="txdf01" title="@ROLEMS.TXDF01" required="true" length="64"/>
        <input type="text" name="define02" expr="txdf02" title="@ROLEMS.TXDF02" required="true" length="64"/>
        <input type="text" name="define03" expr="txdf03" title="@ROLEMS.TXDF03" required="true" length="64"/>
        <input type="text" name="define04" expr="txdf04" title="@ROLEMS.TXDF04" required="true" length="64"/>
